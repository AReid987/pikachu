	<!DOCTYPE html>
<html>
<head>
  <title>Pikachu</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render "/layouts/header" %>

<div>
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}", :class => "alert alert-info" %>
<% end %>
</div>

<div class="container">
<%= yield %>
</div>
</body>
</html>
